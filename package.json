    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^6.0.0",
    "@types/express-session": "^1.17.4",
    "@types/image-thumbnail": "^1.0.1",
    "@types/jest": "^27.0.1",
    "@types/material-ui": "^0.21.12",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
