    "axios": "^0.24.0",
    "babel-plugin-styled-components": "^2.0.2",
    "bcrypt": "^5.0.1",
    "child_process": "^1.0.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-flash": "^0.1.1",
    "express-handlebars": "^6.0.2",
    "express-session": "^1.17.2",
    "firebase-admin": "^10.0.0",
    "formik": "^2.2.9",
    "googleapis": "^92.0.0",
    "image-thumbnail": "^1.0.14",
    "jstat": "^1.9.5",
